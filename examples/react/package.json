    "styled-components": "^2.4.0"
  },
  "devDependencies": {
    "@storybook/react": "^3.3.3",
    "@types/chalk": "^2.2.0",
    "@types/jest": "^22.0.1",
    "@types/node": "^4.0.35",
    "@types/react": "^16.0.34",
    "@types/react-dom": "^16.0.3",
    "@types/storybook__react": "^3.0.6"
  }
}
